        <owl:versionIRI rdf:resource="https://nfdi.fiz-karlsruhe.de/ontology/1.1.1"/>
        <terms:abstract xml:lang="en">The NFDIcore ontology  (prefix nfdicore) describes resouces (datasets, data providers, persons, projects and other entities) in the data domain of NFDI (https://www.nfdi.de/). It serves as the basis for further domain specific ontologies, e.g. nfdi4culture ontology, nfdi4matwerk ontology. Mappings to numerous external vocabularies and ontologies are provided in an extra file. 
                                          Current development is managed via GitHub: https://github.com/ISE-FIZKarlsruhe/nfdicore . Feel free to give feedback, contribute, or raise issues.
                                          </terms:abstract>
        <terms:created>2024-03-08</terms:created>
        <terms:creator rdf:resource="https://nfdi4culture.de/id/E1925"/>
        <terms:creator rdf:resource="https://nfdi4culture.de/id/E2415"/>
        <terms:creator rdf:resource="https://nfdi4culture.de/id/E2416"/>
        <terms:creator rdf:resource="https://nfdi4culture.de/id/E3095"/>
        <terms:creator rdf:resource="https://orcid.org/0000-0001-7192-7143"/>
        <terms:license rdf:resource="https://creativecommons.org/publicdomain/zero/1.0/"/>
        <terms:title>NFDIcore Ontology</terms:title>
        <vann:preferredNamespacePrefix rdf:resource="https://nfdi.fiz-karlsruhe.de/ontology/nfdi"/>
        <vann:preferredNamespaceUri rdf:resource="https://nfdi.fiz-karlsruhe.de/ontology/"/>
