    <!-- https://nfdi.fiz-karlsruhe.de/ontology/Consortium -->

    <owl:Class rdf:about="https://nfdi.fiz-karlsruhe.de/ontology/Consortium">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://nfdi.fiz-karlsruhe.de/ontology/Organization"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://nfdi.fiz-karlsruhe.de/ontology/part"/>
