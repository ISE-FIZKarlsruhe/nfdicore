#    Classes
#################################################################

###  http://nfdi.fiz-karlsruhe.de/ontology/AcademicDiscipline
nfdicore:AcademicDiscipline rdf:type owl:Class ;
                            nfdicore:example wikidata:Q12271 ,
                                             "Architecture, Art History, Musicology, Media Studies, Performing Arts"@en ;
                            rdfs:comment "An academic discipline, a research field or domain of a resource, e.g. a data portal or a filed of study of a person."@en ;
                            rdfs:label "academic discipline"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/City
nfdicore:City rdf:type owl:Class ;
              rdfs:subClassOf nfdicore:Place ;
              nfdicore:example "Berlin, Hannover, Kyiv"@en ;
              rdfs:comment "A city or a town where the thing is located."@en ;
              rdfs:label "city"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/Collection
nfdicore:Collection rdf:type owl:Class ;
                    rdfs:subClassOf schema:Product ;
                    nfdicore:example "RISM catalogue of musical sources"@en ;
                    rdfs:comment "A tangible collection of valuables published or curated by an organisation or a person."@en ;
                    rdfs:label "collection"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/Consortium
nfdicore:Consortium rdf:type owl:Class ;
                    rdfs:subClassOf nfdicore:Organization ;
                    nfdicore:example "NFDI4Culture, NFDI-MatWerk, DataPLANT." ;
                    rdfs:comment "NFDI consortia grouped by a discipline based in a science-led process managed by the German Research Foundation (DFG)."@en ;
                    rdfs:label "consortium"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/Country
nfdicore:Country rdf:type owl:Class ;
                 rdfs:subClassOf nfdicore:Place ;
                 rdfs:comment "A country where the thing is located."@en ;
                 rdfs:label "country"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/DataPortal
nfdicore:DataPortal rdf:type owl:Class ;
