#################################################################
#    Object Properties
#################################################################

###  http://nfdi.fiz-karlsruhe.de/ontology/bestPractice
nfdi:bestPractice rdf:type owl:ObjectProperty ;
                  owl:inverseOf nfdi:bestPracticeOf ;
                  rdfs:domain nfdi:Work ;
                  rdfs:range nfdi:Specification ;
                  rdfs:comment "The property is a relationship between a work and the best practice."@en ;
                  rdfs:label "best practice"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/bestPracticeOf
nfdi:bestPracticeOf rdf:type owl:ObjectProperty ;
                    rdfs:label "best practice of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/contactPoint
nfdi:contactPoint rdf:type owl:ObjectProperty ;
                  owl:inverseOf nfdi:contactPointOf ;
                  rdfs:domain nfdi:Work ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( nfdi:Organization
                                             nfdi:Person
                                           )
                             ] ;
                  rdfs:comment "The property is a relationship between a work and a contact point."@en ;
                  rdfs:label "contact point"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/contactPointOf
nfdi:contactPointOf rdf:type owl:ObjectProperty ;
                    rdfs:label "contact point of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/contributor
nfdi:contributor rdf:type owl:ObjectProperty ;
                 owl:inverseOf nfdi:contributorOf ;
                 rdfs:domain nfdi:Work ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( nfdi:Organization
                                            nfdi:Person
                                          )
                            ] ;
                 rdfs:comment "The property related a work to a person or an organisation responsible for making contributions to the work."@en ;
                 rdfs:label "contributor"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/contributorOf
nfdi:contributorOf rdf:type owl:ObjectProperty .


###  http://nfdi.fiz-karlsruhe.de/ontology/country
nfdi:country rdf:type owl:ObjectProperty ;
             owl:inverseOf nfdi:countryOf ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( nfdi:City
                                         nfdi:FederalState
                                       )
                         ] ;
             rdfs:range nfdi:Country ;
             rdfs:comment "The property relates a city or a state to a country where it is located."@en ;
             rdfs:label "country"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/countryOf
nfdi:countryOf rdf:type owl:ObjectProperty ;
               rdfs:label "country of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/exportFormat
nfdi:exportFormat rdf:type owl:ObjectProperty ;
                  owl:inverseOf nfdi:exportFormatOf ;
                  rdfs:domain nfdi:Work ;
                  rdfs:range nfdi:MediaType ;
                  rdfs:comment "The property relates a work to the format available for the export of a work."@en ;
                  rdfs:label "export format"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/exportFormatOf
nfdi:exportFormatOf rdf:type owl:ObjectProperty ;
                    rdfs:label "export format of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/federalState
nfdi:federalState rdf:type owl:ObjectProperty ;
                  owl:inverseOf nfdi:federalStateOf ;
                  rdfs:domain nfdi:City ;
                  rdfs:range nfdi:FederalState ;
                  rdfs:comment "The property relates a city to a federal state where it is located."@en ;
                  rdfs:label "federal state"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/federalStateOf
nfdi:federalStateOf rdf:type owl:ObjectProperty ;
                    rdfs:label "federal state of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/funder
nfdi:funder rdf:type owl:ObjectProperty ;
            rdfs:domain owl:Thing ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( nfdi:Organization
                                       nfdi:Project
                                     )
                       ] ;
            rdfs:comment "The property relates thing to an organisation or a project that provided funding for it. The subproperties are used to specify more specific funding relationships. "@en ;
            rdfs:label "funder"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/funderOf
nfdi:funderOf rdf:type owl:ObjectProperty ;
              rdfs:label "funder of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/fundingOrganisation
nfdi:fundingOrganisation rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf nfdi:funder ;
                         owl:inverseOf nfdi:fundingOrganisationOf ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( nfdi:Project
                                                     nfdi:Work
                                                   )
                                     ] ;
                         rdfs:range nfdi:Organization ;
                         rdfs:comment "The property relates a work or a project to the organisation that provided funding for it."@en ;
                         rdfs:label "funding organisation"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/fundingOrganisationOf
nfdi:fundingOrganisationOf rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf nfdi:funderOf ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( nfdi:Project
                                                       nfdi:Work
                                                     )
                                       ] ;
                           rdfs:label "funding organisation of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/fundingProject
nfdi:fundingProject rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf nfdi:funder ;
                    owl:inverseOf nfdi:fundingProjectOf ;
                    rdfs:domain nfdi:Work ;
                    rdfs:range nfdi:Project ;
                    rdfs:comment "The property relateds a work to the project that provided funding for it."@en ;
                    rdfs:label "funding project"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/fundingProjectOf
nfdi:fundingProjectOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf nfdi:funderOf ;
                      rdfs:label "funding project of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/instance
nfdi:instance rdf:type owl:ObjectProperty ;
              owl:inverseOf nfdi:instanceOf ;
              rdfs:domain nfdi:Service ;
              rdfs:range nfdi:Service ;
              rdfs:comment "The property is a relationship between an RDM Tool to its running instance."@en ;
              rdfs:label "instance"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/instanceOf
nfdi:instanceOf rdf:type owl:ObjectProperty ;
                rdfs:label "instance of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/license
nfdi:license rdf:type owl:ObjectProperty ;
             owl:inverseOf nfdi:licenseOf ;
             rdfs:domain nfdi:Work ;
             rdfs:range nfdi:License ;
             rdfs:comment "The property is a relationship between a work and a license file." ;
             rdfs:label "license"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/licenseOf
nfdi:licenseOf rdf:type owl:ObjectProperty ;
               rdfs:label "license of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/location
nfdi:location rdf:type owl:ObjectProperty ;
              owl:inverseOf nfdi:locationOf ;
              rdfs:domain owl:Thing ;
              rdfs:range nfdi:Place ;
              rdfs:comment "The property is a relationship between a thing and a place its located at. "@en ;
              rdfs:label "location"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/locationOf
nfdi:locationOf rdf:type owl:ObjectProperty ;
                rdfs:label "location of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/mapping
nfdi:mapping rdf:type owl:ObjectProperty ;
             owl:inverseOf nfdi:mappingOf ;
             rdfs:domain nfdi:Work ;
             rdfs:range nfdi:Mapping ;
             rdfs:comment "The property is a relationship between a a data resource and its various representations, e.g. same resource that exists in different data formats. "@en ;
             rdfs:label "mapping"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/mappingOf
nfdi:mappingOf rdf:type owl:ObjectProperty ;
               rdfs:label "mapping of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/mediaType
nfdi:mediaType rdf:type owl:ObjectProperty ;
               owl:inverseOf nfdi:mediaTypeOf ;
               rdfs:domain nfdi:Work ;
               rdfs:range nfdi:MediaType ;
               rdfs:comment "The property relates a contrbution to its data format."@en ;
               rdfs:label "media type"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/mediaTypeOf
nfdi:mediaTypeOf rdf:type owl:ObjectProperty ;
                 rdfs:label "media type of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/objectType
nfdi:objectType rdf:type owl:ObjectProperty ;
                owl:inverseOf nfdi:objectTypeOf ;
                rdfs:domain nfdi:Collection ;
                rdfs:range nfdi:ObjectType ;
                rdfs:comment "The property relates a data collection to the type of object it contains. "@en ;
                rdfs:label "object type"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/objectTypeOf
nfdi:objectTypeOf rdf:type owl:ObjectProperty ;
                  rdfs:label "object type of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/parentOrganisation
nfdi:parentOrganisation rdf:type owl:ObjectProperty ;
                        owl:inverseOf nfdi:subsidiaryOrganization ;
                        rdfs:domain nfdi:Organization ;
                        rdfs:range nfdi:Organization ;
                        rdfs:comment """The property is a parental relationship between organisations. 
"""@en ;
                        rdfs:label "parent organisation"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/partnerOrganization
nfdi:partnerOrganization rdf:type owl:ObjectProperty ,
                                  owl:SymmetricProperty ;
                         rdfs:domain nfdi:Organization ;
                         rdfs:range nfdi:Organization .


###  http://nfdi.fiz-karlsruhe.de/ontology/programmingLanguage
nfdi:programmingLanguage rdf:type owl:ObjectProperty ;
                         owl:inverseOf nfdi:programmingLanguageOf ;
                         rdfs:domain nfdi:Software ;
                         rdfs:range nfdi:ProgrammingLanguage ;
                         rdfs:comment "The property links software to a programming language its written in. " ;
                         rdfs:label "programming language"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/programmingLanguageOf
nfdi:programmingLanguageOf rdf:type owl:ObjectProperty ;
                           rdfs:label "programming language of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/provisionType
nfdi:provisionType rdf:type owl:ObjectProperty ;
                   rdfs:domain nfdi:ProvidedWork ;
                   rdfs:range nfdi:Work ;
                   rdfs:comment "The property links a work provided by a community to its type."@en ;
                   rdfs:label "provision type"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/publisher
nfdi:publisher rdf:type owl:ObjectProperty ;
               owl:inverseOf nfdi:publisherOf ;
               rdfs:domain nfdi:Work ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( nfdi:Organization
                                          nfdi:Person
                                        )
                          ] ;
               rdfs:comment "The property is a relationship between a resource and an organisation or a person responsible for publishing it."@en ;
               rdfs:label "publisher"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/publisherOf
nfdi:publisherOf rdf:type owl:ObjectProperty ;
                 rdfs:label "publisher of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/relatedProject
nfdi:relatedProject rdf:type owl:ObjectProperty ,
                             owl:SymmetricProperty ;
                    rdfs:domain nfdi:Project ;
                    rdfs:range nfdi:Project ;
                    rdfs:comment "The property is a relationship between related projects. The property is symmetric."@en ;
                    rdfs:label "related project"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/semanticExpressivity
nfdi:semanticExpressivity rdf:type owl:ObjectProperty ;
                          owl:inverseOf nfdi:semanticExpressivityOf ;
                          rdfs:domain nfdi:Work ;
                          rdfs:range nfdi:SemanticExpressivity ;
                          rdfs:comment "The property relates a work to its semantic expressivity spectrum."@en ;
                          rdfs:label "semantic expressivity"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/semanticExpressivityOf
nfdi:semanticExpressivityOf rdf:type owl:ObjectProperty ;
                            rdfs:label "semantic expressivity of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/serviceType
nfdi:serviceType rdf:type owl:ObjectProperty ;
                 owl:inverseOf nfdi:serviceTypeOf ;
                 rdfs:domain nfdi:Service ;
                 rdfs:range nfdi:ServiceType ;
                 rdfs:comment "The property relates a service to its type." ;
                 rdfs:label "service type"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/serviceTypeOf
nfdi:serviceTypeOf rdf:type owl:ObjectProperty ;
                   rdfs:label "service type of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/software
nfdi:software rdf:type owl:ObjectProperty ;
              owl:inverseOf nfdi:sofwareOf ;
              rdfs:domain nfdi:Work ;
              rdfs:range nfdi:Software ;
              rdfs:comment "The property is a relationship between a work and a software it uses."@en ;
              rdfs:label "software"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/softwareOf
nfdi:softwareOf rdf:type owl:ObjectProperty ;
                rdfs:label "software of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/sofwareOf
nfdi:sofwareOf rdf:type owl:ObjectProperty ;
               rdfs:label "sofware of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/sparqlEndpoint
nfdi:sparqlEndpoint rdf:type owl:ObjectProperty ;
                    owl:inverseOf nfdi:sparqlEndpointOf ;
                    rdfs:domain nfdi:Work ;
                    rdfs:range nfdi:Work ;
                    rdfs:comment "The property relates a data resource to its SPARQL Endpoint. "@en ;
                    rdfs:label "sparql endpoint"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/sparqlEndpointOf
nfdi:sparqlEndpointOf rdf:type owl:ObjectProperty ;
                      rdfs:label "sparql endpoint of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/specification
nfdi:specification rdf:type owl:ObjectProperty ;
                   owl:inverseOf nfdi:specificationOf ;
                   rdfs:domain nfdi:Work ;
                   rdfs:range nfdi:Specification ;
                   rdfs:comment "The property is a relationship between a resource and the international standard or norm."@en ;
                   rdfs:label "specification"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/specificationOf
nfdi:specificationOf rdf:type owl:ObjectProperty ;
                     rdfs:label "specification of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/standard
nfdi:standard rdf:type owl:ObjectProperty ;
              owl:inverseOf nfdi:standardOf ;
              rdfs:domain nfdi:Work ;
              rdfs:range nfdi:Standard ;
              rdfs:comment "The property relates a work to the standard it relies on."@en ;
              rdfs:label "standard"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/standardOf
nfdi:standardOf rdf:type owl:ObjectProperty ;
                rdfs:label "standard of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/subjectArea
nfdi:subjectArea rdf:type owl:ObjectProperty ;
                 owl:inverseOf nfdi:subjectAreaOf ;
                 rdfs:domain nfdi:Work ;
                 rdfs:range nfdi:AcademicDiscipline ;
                 rdfs:comment "The property is a relationship between a work and its academic discipline, subject area or domain."@en ;
                 rdfs:label "subject area"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/subjectAreaOf
nfdi:subjectAreaOf rdf:type owl:ObjectProperty ;
                   rdfs:label "subject area of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/subsidiaryOrganization
nfdi:subsidiaryOrganization rdf:type owl:ObjectProperty ;
                            rdfs:label "subsidiary organization"@en .


