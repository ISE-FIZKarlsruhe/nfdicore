nfdi:technology rdf:type owl:ObjectProperty ;
                owl:inverseOf nfdi:technologyOf ;
                rdfs:domain nfdi:Work ;
                rdfs:range nfdi:TechnologicalMeans ;
                rdfs:comment "The property is a relationship between a resource and a technology it uses. "@en ;
                rdfs:label "technology"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/technologyOf
nfdi:technologyOf rdf:type owl:ObjectProperty ;
                  rdfs:label "technology of"@en .


#################################################################
#    Data properties
#################################################################

###  http://nfdi.fiz-karlsruhe.de/ontology/areaCode
nfdi:areaCode rdf:type owl:DatatypeProperty ;
              rdfs:domain nfdi:Organization ;
              rdfs:range rdfs:Literal ;
              rdfs:label "area code"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/emailAddress
nfdi:emailAddress rdf:type owl:DatatypeProperty ;
                  rdfs:domain nfdi:Organization ;
                  rdfs:range rdfs:Literal ;
                  rdfs:label "email address"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/url
nfdi:url rdf:type owl:DatatypeProperty ;
         rdfs:domain owl:Thing ;
         rdfs:range xsd:anyURI ;
         rdfs:comment "URL of a thing"@en ;
         rdfs:label "url"@en .


#################################################################
#    Classes
#################################################################

###  http://nfdi.fiz-karlsruhe.de/ontology/AcademicDiscipline
nfdi:AcademicDiscipline rdf:type owl:Class ;
                        nfdi:example wikidata:Q12271 ,
                                     "Architecture, Art History, Musicology, Media Studies, Performing Arts"@en ;
                        rdfs:comment "An academic discipline, a research field or domain of a resource, e.g. a data portal or a filed of study of a person."@en ;
                        rdfs:label "academic discipline"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/City
nfdi:City rdf:type owl:Class ;
          rdfs:subClassOf nfdi:Place ;
          nfdi:example "Berlin, Hannover, Kyiv"@en ;
          rdfs:comment "A city or a town where the thing is located."@en ;
          rdfs:label "city"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/Collection
nfdi:Collection rdf:type owl:Class ;
                rdfs:subClassOf schema:Product ;
                nfdi:example "RISM catalogue of musical sources"@en ;
                rdfs:comment "A tangible collection of valuables published or curated by an organisation or a person."@en ;
                rdfs:label "collection"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/Consortium
nfdi:Consortium rdf:type owl:Class ;
                rdfs:subClassOf nfdi:Organization ;
                nfdi:example "NFDI4Culture, NFDI-MatWerk, DataPLANT." ;
                rdfs:comment "NFDI consortia grouped by a discipline based in a science-led process managed by the German Research Foundation (DFG)."@en ;
                rdfs:label "consortium"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/Country
nfdi:Country rdf:type owl:Class ;
             rdfs:subClassOf nfdi:Place ;
             rdfs:comment "A country where the thing is located."@en ;
             rdfs:label "country"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/DataPortal
nfdi:DataPortal rdf:type owl:Class ;
                rdfs:subClassOf schema:Service ;
                nfdi:example "Bach digital, KultSam, MusiXplora"@en ;
                rdfs:comment "An online platform that provides access to data collections and datasets."@en ;
                rdfs:label "data portal"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/DevelopmentWork
nfdi:DevelopmentWork rdf:type owl:Class ;
                     nfdi:example "Development of a controlled vocabulary for audivisual material in publication and research data; Extension of the AAT vocabulary for the history of architecture"@en ;
                     rdfs:comment "A resource that identifies an ongoing development of standards, services, data models, vocabularies, ontologies, etc."@en ;
                     rdfs:label "development work"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/FederalState
nfdi:FederalState rdf:type owl:Class ;
                  rdfs:subClassOf nfdi:Place ;
                  nfdi:example "Sachsen, Berlin"@de ;
                  rdfs:comment "A federal state where the thing is located."@en ;
                  rdfs:label "federal state"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/License
nfdi:License rdf:type owl:Class ;
             nfdi:example "cc-by-3, Digital Peer Publishing Lizenz"@en ;
             rdfs:comment "A document that provides legal guidelines for the use of a resource, e.g. a software."@en ;
             rdfs:label "license"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/Mapping
nfdi:Mapping rdf:type owl:Class ;
             nfdi:example "Bach Digital Mapping, WeGA Gudielines"@en ;
