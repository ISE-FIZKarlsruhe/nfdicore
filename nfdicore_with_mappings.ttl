###  http://nfdi.fiz-karlsruhe.de/ontology#representedCollection
:representedCollection rdf:type owl:ObjectProperty ;
                       owl:inverseOf :representedCollectionOf ;
                       rdfs:label "represented collection of"@en .

###  http://nfdi.fiz-karlsruhe.de/ontology#representedCollectionOf
:representedCollectionOf rdf:type owl:ObjectProperty ;
                         rdfs:comment "The property is a relationship between a dataset and a collection it represents."@en ;
                         rdfs:label "represented collection"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/bestPractice
nfdicore:bestPractice rdf:type owl:ObjectProperty ;
                      owl:inverseOf nfdicore:bestPracticeOf ;
                      rdfs:domain nfdicore:Resource ;
                      rdfs:range nfdicore:Specification ;
                      rdfs:comment "The property is a relationship between a resource and the best practice."@en ;
                      rdfs:label "best practice"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/bestPracticeOf
nfdicore:bestPracticeOf rdf:type owl:ObjectProperty ;
                        rdfs:label "best practice of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/contactPoint
nfdicore:contactPoint rdf:type owl:ObjectProperty ;
                      owl:equivalentProperty dcat:contactPoint ;
                      owl:inverseOf nfdicore:contactPointOf ;
                      rdfs:domain nfdicore:Resource ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( nfdicore:Organization
                                                 nfdicore:Person
                                               )
                                 ] ;
                      rdfs:comment "The property is a relationship between a resource and a contact point."@en ;
                      rdfs:label "contact point"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/contactPointOf
nfdicore:contactPointOf rdf:type owl:ObjectProperty ;
                        rdfs:label "contact point of"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/contributor
nfdicore:contributor rdf:type owl:ObjectProperty ;
                     owl:equivalentProperty dcterms:contributor ;
                     owl:inverseOf nfdicore:contributorOf ;
                     rdfs:domain nfdicore:Resource ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( nfdicore:Organization
                                                nfdicore:Person
                                              )
                                ] ;
                     rdfs:comment "The property related a resource to a person or an organisation responsible for making contributions to a resource."@en ;
                     rdfs:label "contributor"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/contributorOf
nfdicore:contributorOf rdf:type owl:ObjectProperty .


###  http://nfdi.fiz-karlsruhe.de/ontology/country
nfdicore:country rdf:type owl:ObjectProperty ;
                 owl:inverseOf nfdicore:countryOf ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( nfdicore:City
                                             nfdicore:FederalState
                                           )
                             ] ;
                 rdfs:range nfdicore:Country ;
                 rdfs:comment "The property relates a city or a state to a country where it is located."@en ;
                 rdfs:label "country"@en .


###  http://nfdi.fiz-karlsruhe.de/ontology/countryOf
nfdicore:countryOf rdf:type owl:ObjectProperty ;
                   rdfs:label "country of"@en .


