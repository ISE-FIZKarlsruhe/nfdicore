                             owl:equivalentProperty frapo:hasFundingAgency ;
                             rdfs:subPropertyOf nfdicore:funder ;
                             owl:inverseOf nfdicore:fundingOrganisationOf ;
                             rdfs:domain [ rdf:type owl:Class ;
                                           owl:unionOf ( nfdicore:Project
                                                         nfdicore:Resource
                                                       )
                                         ] ;
                             rdfs:range nfdicore:Organization ;
                             rdfs:comment "The property relates a resource or a project to the organisation that provided funding for it."@en ;
                             rdfs:label "funding organisation"@en .


