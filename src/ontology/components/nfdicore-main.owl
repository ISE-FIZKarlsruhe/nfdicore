AnnotationAssertion(rdfs:isDefinedBy :AcademicDiscipline <https://nfdi.fiz-karlsruhe.de/ontology>)
AnnotationAssertion(rdfs:label :AcademicDiscipline "academic discipline"@en)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :AcademicDiscipline <https://www.wikidata.org/entity/Q12271>)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#example> :AcademicDiscipline "Architecture, Art History, Musicology, Media Studies, Performing Arts"@en)
SubClassOf(:AcademicDiscipline <http://purl.obolibrary.org/obo/BFO_0000019>)

